{
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "semi": "error",
    "object-curly-spacing": "off",
    "linebreak-style": "off",
    "eol-last": "off",
    "max-len": "off"
  }
}